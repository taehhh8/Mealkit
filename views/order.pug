      //에스크로 사용
      document.order_info.escw_used.value = "Y";
      document.order_info.pay_mod.value = "Y";
      }
      }
      function create_goodInfo( goodInfos )
      {
      if( goodInfos && goodInfos.length > 0 ){
      var chr30 = String.fromCharCode(30);
      var chr31 = String.fromCharCode(31);
      var good_info = "";
      for (var i = 0; i < goodInfos.length; i++) {
      if( i > 0 ) good_info += chr30;
      good_info += "seq="+ (i+1) + chr31;
      good_info += "ordr_numb="+ goodInfos[i].OD_ORDER_DETL_ID + chr31;
      good_info += "good_name="+ kcpStringReplace(goodInfos[i].ITEM_NAME) + chr31;
      good_info += "good_cntx="+ goodInfos[i].TOTAL_QTY + chr31;
      good_info += "good_amtx=" + goodInfos[i].PAID_PRICE;
      }
      document.order_info.good_info.value = good_info;
      }
      }
      function kcpStringReplace(str){
      //kcp 특수문자 안되는거
      //콤마 	엠퍼센트 	세미콜론 	뉴 라인 	역 슬래쉬 	파이프 라인 	작은 따옴표 	큰 따옴표 	부등호
      //, 	& 		; 		\\n 		\ 		| 		‘ 		“ 		<
      return str.replace(/,/g,".")
      .replace(/&/g,".")
      .replace(/;/g,".")
      .replace(/\\n/g," ")
      .replace(/\\\/g," ")
      .replace(/\\|/g," ")
      .replace(/\\‘/g," ")
      .replace(/\\“/g," ")
      .replace(/\\"/g," ")
      .replace(/\\</g," ");
      }
