      var npReturnURL	= "https://www.greating.co.kr/pg/naverpay/approval?d=p&MallReserved="+npMRVal;
      oPay = Naver.Pay.open({
      "merchantPayKey"	: bindData.ODID
      , "productName"		: bindData.ITEMS_INFO[0].ITEM_NAME
      , "totalPayAmount"	: bindData.PAID_PRICE
      , "taxScopeAmount"	: bindData.TAXABLE_PRICE
      , "taxExScopeAmount": bindData.NON_TAXABLE_PRICE
      , "productCount"	: bindData.ITEMS_COUNT
      , "returnUrl"		: npReturnURL
      , "productItems"	: naverProductItems
      }); // end Naver.Pay.open
      }
