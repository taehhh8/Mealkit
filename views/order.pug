      "mode" 			: "production"
      , "clientId"		: naverCreateClientId
      , "payType"		: "normal"
      , "openType"		: "popup"
      }); // end Naver.Pay.create
      var naverProductItems = new Array();
      for(var i=0; i<bindData.ITEMS_INFO.length; i++){
      var productItem = new Object();
      productItem.categoryType 	= "PRODUCT";
      productItem.categoryId 		= "GENERAL";
      productItem.uid 			= bindData.ITEMS_INFO[i].IL_ITEM_ID+"";
      productItem.name 			= bindData.ITEMS_INFO[i].ITEM_NAME;
      productItem.count 			= bindData.ITEMS_INFO[i].QTY;
      var isPush = true;
      for(var j=0; j<naverProductItems.length; j++){
      if( naverProductItems[j].uid == productItem.uid ){
      naverProductItems[j].count = naverProductItems[j].count + bindData.ITEMS_INFO[i].QTY;
      isPush = false;
      break;
      }
      }
      if(isPush){
      naverProductItems.push(productItem);
      }
      }
      //console.warn("bindData.ITEMS_INFO[0].ITEM_NAME::"+ bindData.ITEMS_INFO[0].ITEM_NAME );
      //console.warn("naverProductItems::"+ JSON.stringify(naverProductItems) );
