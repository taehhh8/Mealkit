      document.order_info.van_code.value = "SCWB";
      document.order_info.ActionResult.value = "tpnt";
      } else if (psPayGrpId == "scbl") {//도서상품권
      document.order_info.pay_method.value = "GIFT";
      document.order_info.van_code.value = "SCBL";
      document.order_info.ActionResult.value = "scbl";
      } else if (psPayGrpId == "sccl") {//문화상품권
      document.order_info.pay_method.value = "GIFT";
      document.order_info.van_code.value = "SCCL";
      document.order_info.ActionResult.value = "sccl";
      } else if (psPayGrpId == "schm") {//해피머니
      document.order_info.pay_method.value = "GIFT";
      document.order_info.van_code.value = "SCHM";
      document.order_info.ActionResult.value = "schm";
