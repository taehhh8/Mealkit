      function jsf__chk_type(psPayGrpId) {
      if (pgRegularYn == "Y" || pgRegularYn == "C") {//정기 과금
      document.order_info.pay_method.value = "AUTH";
      document.order_info.ActionResult.value = "batch";
      } else if (psPayGrpId == 7) {//페이코
      document.order_info.pay_method.value = "CARD";
      document.order_info.ActionResult.value = "card";
      document.order_info.payco_direct.value = "Y";
      } else if (psPayGrpId == 2) {//신용카드
      document.order_info.pay_method.value = "CARD";
      document.order_info.ActionResult.value = "card";
      } else if (psPayGrpId == 4) {//계좌이체
      document.order_info.pay_method.value = "BANK";
      document.order_info.ActionResult.value = "acnt";
      } else if (psPayGrpId == 3) {//가상계좌
      document.order_info.pay_method.value = "VCNT";
      document.order_info.ActionResult.value = "vcnt";
      //에스크로 사용 가상계좌는 사용 안함
      //document.order_info.escw_used.value = "Y";
      //document.order_info.pay_mod.value = "Y";
      } else if (psPayGrpId == 5) {//휴대폰
      document.order_info.pay_method.value = "MOBX";
      document.order_info.ActionResult.value = "mobx";
      } else if (psPayGrpId == "ocb") {//OK캐쉬백
